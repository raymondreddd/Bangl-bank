<%- include('./partials/header') %>

<br><br>
<h1>Hell</h1>
<h3><%= trans.length %></h3>

<table class="table table-striped table-dark table-bordered">
    <thead class="thead-dark">
      <tr class='transaction'>
        <th scope="col" class='TFrom' >FROM</th>
        <th scope="col" class='TTO'>TO</th>
        <th scope="col" class='TAmount'>AMOUNT</th>
        <th scope="col" class='TDate'>DATE</th>
      </tr>
    </thead>
    <tbody>
        
            <% trans.forEach((t) => {%>
                <tr class='transaction'>
                    <td class='TFrom'><%= t.from %></td>
                    <td class='TTO'><%= t.to %></td>
                    <td class='TAmount'><%= t.amount %></td>
                    <td class='TDate'><%= t.date %></td>
                </tr>
            <% }) %> 
    </tbody>
  </table>

<%- include('./partials/footer') %>